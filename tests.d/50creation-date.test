#!/bin/sh

create () {
    cd $1
    set -e
    touch 'creation-date-test'
}


verify () {
    cd $1
    set -e
    test "`GetFileInfo -d $2/creation-date-test`" = \
         "`GetFileInfo -d ./creation-date-test`"
}

clean () {
    true
}

compare () {
    set -e
    test "`GetFileInfo -P -d $1`" = "`GetFileInfo -P -d $2`"
}
